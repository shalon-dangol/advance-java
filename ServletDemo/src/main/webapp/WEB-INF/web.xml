<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>MyFirstServlet</servlet-name>
        <servlet-class>MyServlets.MyFirstServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>itemSave</servlet-name>
        <servlet-class>backend.itemSave</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllItem</servlet-name>
        <servlet-class>backend.GetAllItem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteItem</servlet-name>
        <servlet-class>backend.DeleteItem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveUser</servlet-name>
        <servlet-class>backend.SaveUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayUser</servlet-name>
        <servlet-class>backend.DisplayUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyFirstServlet</servlet-name>
        <url-pattern>/firstservlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>newone</servlet-name>
        <servlet-class>MyServlets.HttpServletDemo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>newone</servlet-name>
        <url-pattern>/newone</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>generic</servlet-name>
        <servlet-class>MyServlets.GenericServletDemo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>generic</servlet-name>
        <url-pattern>/generic</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>itemSave</servlet-name>
        <url-pattern>/itemSave</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllItem</servlet-name>
        <url-pattern>/GetAllItem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteItem</servlet-name>
        <url-pattern>/DeleteItem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveUser</servlet-name>
        <url-pattern>/SaveUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayUser</servlet-name>
        <url-pattern>/DisplayUser</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
